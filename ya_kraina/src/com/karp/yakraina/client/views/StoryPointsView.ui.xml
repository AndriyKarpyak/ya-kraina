<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:ya="urn:import:com.karp.yakraina.client.widgets">


	<g:HTMLPanel addStyleNames="storySummariesView">
		
		<g:HTMLPanel addStyleNames="storyPointsView" ui:field="pointsPanel">
			<g:HTML addStyleNames="l1" />
			<g:HTML addStyleNames="l2" />

			<g:HTML addStyleNames="storyPointsView-StoryName" ui:field="storyNameLabel" />
	
			<g:HTML addStyleNames="storyPointsView-StorySummaryText" ui:field="storySummaryText" />

			<g:InlineLabel>ВАШ РЕЗУЛЬТАТ</g:InlineLabel>
	
			<g:HTMLPanel addStyleNames="storyPointsView-ResultDisplay">
	
				<g:Image ui:field="storyIcon" />
				<g:HTMLPanel addStyleNames="storyPointsView-ResultText">
					<g:InlineLabel ui:field="pointsText" />
					<g:InlineLabel>БАЛІВ</g:InlineLabel>
				</g:HTMLPanel>
	
			</g:HTMLPanel>
			
			<ya:MatteBytton ui:field="button_Accept" />
			
		</g:HTMLPanel>
		
		<g:HTMLPanel addStyleNames="singleSummaryPanel" ui:field="singleSummaryPanel">
			<g:HTML addStyleNames="l1" />
			<g:HTML addStyleNames="l2" />
			
			<g:HTMLPanel addStyleNames="singleSummaryTextPanel" >
				<g:Image addStyleNames="emoji" ui:field="emoji" />
				<g:HTML addStyleNames="singleSummaryText" ui:field="singleSummaryText" />
			</g:HTMLPanel>
			
			<ya:MatteBytton ui:field="button_Next"/>		
		</g:HTMLPanel>
	</g:HTMLPanel>
</ui:UiBinder> 