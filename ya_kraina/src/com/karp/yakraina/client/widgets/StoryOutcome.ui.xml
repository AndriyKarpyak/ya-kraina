<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:ya="urn:import:com.karp.yakraina.client.widgets">
	
		<g:HTMLPanel addStyleNames="ya-StoryOutcome" ui:field="pointsPanel">
			<g:Image url="images/bg-summaries.png" />

			<g:HTMLPanel addStyleNames="foreground">
				<g:HTML addStyleNames="storyName" ui:field="storyNameLabel" />
				<g:HTML addStyleNames="storySummaryText" ui:field="storySummaryText" />
				<g:InlineLabel>ВАШ РЕЗУЛЬТАТ</g:InlineLabel>
				<g:HTMLPanel addStyleNames="resultDisplay">
					<g:Image url="images/bg-result.svg" />
					
					<g:HTMLPanel addStyleNames="foreground">
						<g:Image ui:field="storyIcon" />
						<g:InlineLabel ui:field="pointsText" />
					</g:HTMLPanel>
				</g:HTMLPanel>
				<ya:MatteButton ui:field="button_Accept" />
			</g:HTMLPanel>
		</g:HTMLPanel>
</ui:UiBinder> 